{
    "name": "Kaltura Downloader",
    "version": "1.0",
    "description": "Build an Extension!",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*.kaltura.com/*"],
            "js": ["sentinel.js"],
            "all_frames": true
        }
    ],
    "manifest_version": 2
}
